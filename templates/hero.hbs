    <label>Armor</label>
    <div class="totals">
      <div>
        <span>{{system.armor.head}}</span>
        <label>Head (8)</label>
      </div>
      <div>
        <span>{{system.armor.body}}</span>
        <label>Body (5-7)</label>
      </div>
      <div>
        <span>{{system.armor.arms}}</span>
        <label>Arms (3-4)</label>
      </div>
      <div>
        <span>{{system.armor.legs}}</span>
        <label>Legs (1-2)</label>
      </div>
    </div>
    <ul>
      {{#with armor}}
        <li data-action="editItem" data-uuid="{{head.uuid}}">
          <span>{{head.name}}
            {{#if head}}({{head.system.value}}){{/if}}</span>
          <span>Head</span>
        </li>
        <li data-action="editItem" data-uuid="{{body.uuid}}">
          <span>{{body.name}}
            {{#if body}}({{body.system.value}}){{/if}}</span>
          <span>Body</span>
        </li>
        <li data-action="editItem" data-uuid="{{arms.uuid}}">
          <span>{{arms.name}}
            {{#if arms}}({{arms.system.value}}){{/if}}</span>
          <span>Arms</span>
        </li>
        <li data-action="editItem" data-uuid="{{legs.uuid}}">
          <span>{{legs.name}}
            {{#if legs}}({{legs.system.value}}){{/if}}</span>
          <span>Legs</span>
        </li>
        <li data-action="editItem" data-uuid="{{shield.uuid}}">
          <span>{{shield.name}} {{#if shield}}({{shield.system.locations}}
              {{shield.system.value}}){{/if}}</span>
          <span>Shield</span>
        </li>
      {{/with}}
    </ul>
  </div>
