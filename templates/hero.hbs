      {{#each gear as |item|}}
        <li data-action="editItem" data-uuid="{{item.uuid}}">
          <span>{{item.name}}{{#if (eq item.type "ammo")}}
              ({{item.system.quantity}}){{/if}}</span><span>{{add @index 1}}</span>
        </li>
      {{/each}}
    </ul>
  </div>
  <div class="notes-section">
    <label>Notes</label>
    <div class="form-field">
      {{formInput system.schema.fields.notes value=system.notes enriched=enriched.system.notes toggled=true}}
