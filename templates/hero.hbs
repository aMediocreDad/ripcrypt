  <div class="aura">
    <label>Aura</label>
    {{formField system.schema.fields.aura.fields.value value=system.aura.value localize=true}}
    {{formField system.schema.fields.aura.fields.max value=system.aura.max localize=true}}
  </div>
  <div class="resources">
    {{formField system.schema.fields.resources.fields.gold value=system.resources.gold localize=true}}
    {{formField system.schema.fields.resources.fields.silver value=system.resources.silver localize=true}}
    {{formField system.schema.fields.resources.fields.copper value=system.resources.copper localize=true}}
  </div>
  <div class="gear" data-drop-type="gear">
    <label><span>Gear</span><span>Slots</span></label>
